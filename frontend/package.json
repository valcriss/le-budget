{
  "name": "le-budget",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "eslint --ext .ts,.html src/ --max-warnings=0",
  "format": "prettier --write \"src/**/*.{ts,html,css,json,md}\"",
  "format:html": "prettier --write \"src/**/*.html\" && node tools/collapse-html-openings.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --runInBand",
    "test:watch": "jest --watch"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
  "@angular/cdk": "^20.2.9",
  "@angular/common": "^20.3.9",
  "@angular/compiler": "^20.3.9",
  "@angular/core": "^20.3.9",
  "@angular/forms": "^20.3.9",
  "@angular/platform-browser": "^20.3.9",
  "@angular/platform-browser-dynamic": "^20.3.9",
  "@angular/animations": "20.3.9",
  "@angular/router": "^20.3.9",
    "@fortawesome/angular-fontawesome": "^3.0.0",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@ng-select/ng-select": "^20.5.0",
    "@tailwindcss/postcss": "^4.1.14",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.14",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
  "eslint": "^9.37.0",
  "@typescript-eslint/parser": "^8.46.1",
  "@typescript-eslint/eslint-plugin": "^8.46.1",
  "eslint-config-prettier": "^10.1.8",
  "prettier": "^3.6.2",
  "rimraf": "^4.0.1",
  "glob": "^9.3.5",
    "@angular/build": "^20.3.8",
    "@angular/cli": "^20.3.8",
    "@angular/compiler-cli": "^20.3.9",
    "@types/jest": "^30.0.0",
    "eslint-plugin-jest": "^28.8.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-preset-angular": "^15.0.3",
    "jsdom": "^26.0.0",
    "ts-jest": "^29.4.5",
    "typescript": "~5.9.2"
  }
  ,
  "overrides": {
    "glob": "^9.3.5",
    "rimraf": "^4.0.1"
  }
}
